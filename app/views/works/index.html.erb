<p id="notice"><%= notice %></p>

<h1>Listing Works</h1>

<table>
  <thead>
    <tr>
      <th>Artwork</th>
      <th colspan="3"></th>
      <th>Artist</th>
      <th colspan="3"></th>
      <th>ArtistBio</th>
      <th colspan="3"></th>
      <th>Date</th>
      <th colspan="3"></th>
      <th>Medium</th>
      <th colspan="3"></th>
      <th>Dimensions</th>
      <th colspan="3"></th>
      <th>MoMANumber</th>
      <th colspan="3"></th>
      <th>Classification</th>
      <th colspan="3"></th>
      <th>Department</th>
      <th colspan="3"></th>
      <th>ObjectID</th>
      <th colspan="3"></th>
      <th>URL</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @works.each do |work| %>
      <tr>
        <td><%= work.artwork %></td>
        <td><%= work.artist %></td>
        <td><%= work.artistbio %></td>
        <td><%= work.date %></td>
        <td><%= work.medium %></td>
        <td><%= work.dimensions %></td>
        <td><%= work.momanumber %></td>
        <td><%= work.classification %></td>
        <td><%= work.department%></td>
        <td><%= work.objectid%></td>
        <td><%= work.url%></td>
        <td><%= link_to 'Show', work %></td>
        <td><%= link_to 'Edit', edit_work_path(work) %></td>
        <td><%= link_to 'Destroy', work, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work', new_work_path %>
